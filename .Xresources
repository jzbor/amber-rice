Xft.dpi:       96
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault


!! My theme
#define myBackground			#0f1418
#define myBackgroundTrans		#e00f1480
#define myForeground			#ebdbb2
#define mySelector			#b83e1e
#define myUrgent			#d78272

*themeBackground:		  myBackground
*themeBackgroundTrans:		  myBackgroundTrans
*themeForeground:		  myForeground
*themeSelector:			  mySelector
*themeUrgent:			  myUrgent


*background:                      myBackground
*foreground:                      myForeground
*fading:                          8
*fadeColor:                       black
*cursorColor:                     #1ABB9B
*pointerColorBackground:          #2B2C2B
*pointerColorForeground:          #16A085

!! DWM values
*normbgcolor:			myBackground
*normbordercolor:		myBackground
*normfgcolor:			myForeground
*selbgcolor:			myForeground
*selbordercolor:		myForeground
*selfgcolor:			myBackground

!! X clock
Clock*Background:		myBackground
Clock*BorderColor:		myForeground
Clock*hour:			myForeground
Clock*jewel:			myForeground
Clock*minute:			myForeground

XClock*foreground: #fbfbfb
XClock*background:		myBackground
XClock*update:			1
XClock*geometry:		210x210+20+40
XClock*majorColor:		mySelector
XClock*minorColor:		myForeground
XClock*hourColor:		myForeground
XClock*minuteColor:		myForeground
XClock*secondColor:		myForeground
XClock*render:			true
XClock*chime:			true


!! Black + DarkGrey
*color0:			#282828
*color8:			#928374
