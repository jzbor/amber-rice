Xcursor.theme: Breeze_Snow
Xcursor.size:                     16


XTerm*background:	 myForeground
XTerm*foreground:        myBackground
XTerm*pointerColor:      myForeground
XTerm*faceName:          Fixed
XTerm*faceSize:          11
XTerm*reverseVideo:      on
XTerm*selectToClipboard: true
UXTerm*font:		 -misc-fixed-medium-r-normal--13-120-75-75-c-80-iso10646-1


! Enabled modi
rofi.modi:                           ssh,window,drun,run
! Window width
rofi.width:                          50
! Number of lines
rofi.lines:                          6
! Number of columns
rofi.columns:                        1
! Font to use
rofi.font:			    Fira Code 14
! Border width
rofi.bw:                             2
! Location on screen
! rofi.location:                       0
! Padding
rofi.padding:                        20
! Y-offset relative to location
! rofi.yoffset:                        0
! X-offset relative to location
! rofi.xoffset:                        0
! Always show number of lines
! rofi.fixed-num-lines:                true
! Whether to load and show icons
rofi.show-icons:                     true
! Terminal to use
rofi.terminal:                       kitty
! Ssh client to use
! rofi.ssh-client:                     ssh
! Ssh command to execute
! rofi.ssh-command:                    {terminal} -e {ssh-client} {host} [-p {port}]
! Run command to execute
rofi.run-command:                    bash -c {cmd}
! Command to get extra run targets
! rofi.run-list-command:
! Run command to execute that runs in shell
! rofi.run-shell-command:              {terminal} -e {cmd}
! Command executed on accep-entry-custom for window modus
! rofi.window-command:                 wmctrl -i -R {window}
! Window fields to match in window mode
! rofi.window-match-fields:            all
! Theme to use to look for icons
! rofi.icon-theme:
! Desktop entry fields to match in drun
! rofi.drun-match-fields:              name,generic,exec,categories
! Desktop entry show actions.
! rofi.drun-show-actions:              false
! DRUN format string. (Supports: generic,name,comment,exec,categories)
rofi.drun-display-format:            {name} [<span weight='light' size='small'><i>({generic})</i></span>]
! Disable history in run/ssh
rofi.disable-history:                true
! Programs ignored for history
rofi.ignored-prefixes: firefox-private
! Use sorting
! rofi.sort:                           false
! Choose the strategy used for sorting: normal (levenshtein) or fzf.
! rofi.sorting-method:
! Set case-sensitivity
rofi.case-sensitive:                 false
! Cycle through the results list
! rofi.cycle:                          true
! Enable sidebar-mode
! rofi.sidebar-mode:                   false
! Row height (in chars)
! rofi.eh:                             1
! Enable auto select mode
! rofi.auto-select:                    false
! Parse hosts file for ssh mode
! rofi.parse-hosts:                    false
! Parse known_hosts file for ssh mode
! rofi.parse-known-hosts:              true
! Set the modi to combine in combi mode
! rofi.combi-modi:                     window,run
! Set the matching algorithm. (normal, regex, glob, fuzzy)
! rofi.matching:                       normal
! Tokenize input string
! rofi.tokenize:                       true
! Monitor id to show on
! rofi.m:                              -5
! Margin between rows *DEPRECATED*
! rofi.line-margin:                    10
! Padding within rows *DEPRECATED*
rofi.line-padding:                   5
! Pre-set filter
! rofi.filter:
! Separator style (none, dash, solid) *DEPRECATED*
! rofi.separator-style:                dash
! Hide scroll-bar *DEPRECATED*
! rofi.hide-scrollbar:                 false
! Fullscreen
! rofi.fullscreen:                     false
! Fake transparency *DEPRECATED*
! rofi.fake-transparency:              false
! DPI
! rofi.dpi:                            -1
! Threads to use for string matching
! rofi.threads:                        0
! Scrollbar width *DEPRECATED*
! rofi.scrollbar-width:                8
! Scrolling method. (0: Page, 1: Centered)
! rofi.scroll-method:                  0
! Background to use for fake transparency. (background or screenshot) *DEPRECATED*
! rofi.fake-background:                screenshot
! Window Format. w (desktop name), t (title), n (name), r (role), c (class)
! rofi.window-format:                  {w}    {c}   {t}
! Click outside the window to exit
! rofi.click-to-exit:                  true
! Indicate how it match by underlining it.
! rofi.show-match:                     true
! New style theme file
! rofi.theme:
! Color scheme for normal row
rofi.color-normal:	myBackground,    myForeground,	myBackground,    myForeground,	myBackground
! Color scheme for urgent row
rofi.color-urgent:	myBackground,    myUrgent,	myBackground,    myUrgent,	myBackground
! Color scheme for active row
rofi.color-active:	myBackground,    mySelector,	myBackground,    mySelector,	myBackground
! Color scheme window
rofi.color-window:	myBackground, myForeground, myBackground
! Max history size (WARNING: can cause slowdowns when set to high).
! rofi.max-history-size:               25
! Hide the prefix mode prefix on the combi view.
! rofi.combi-hide-mode-prefix:         false
! Set the character used to negate the matching. ('\0' to disable)
rofi.matching-negate-char:           \
! Directory where history and temporary files are stored.
! rofi.cache-dir:
! Pidfile location
! rofi.pid:                            /run/user/1000/rofi.pid
! The display name of this browser
! rofi.display-window:
! The display name of this browser
! rofi.display-windowcd:
! The display name of this browser
! rofi.display-run:
! The display name of this browser
! rofi.display-ssh:
! The display name of this browser
